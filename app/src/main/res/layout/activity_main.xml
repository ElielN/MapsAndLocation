<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button_CdNatal"
        android:layout_width="411dp"
        android:layout_height="57dp"
        android:layout_marginTop="86dp"
        android:text="Minha casa na cidade natal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_Vicosa"
        android:layout_width="411dp"
        android:layout_height="57dp"
        android:layout_marginTop="143dp"
        android:text="Minha casa em Viçosa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_Departamento"
        android:layout_width="411dp"
        android:layout_height="57dp"
        android:layout_marginTop="200dp"
        android:text="Meu departamento"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_Fechar"
        android:layout_width="411dp"
        android:layout_height="57dp"
        android:layout_marginTop="257dp"
        android:text="Fechar aplicação"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>